root = true

[*]
indent_style = space
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
max_line_length = off

[*.rst]
indent_size = 4

[*.bat]
end_of_line = crlf

[*.{yaml,yml}]
indent_size = 2

[*.html]
indent_size = 2

[*.css]
indent_size = 2

[*.js]
indent_size = 2
